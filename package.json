{
  "name": "svelte-web3",
  "version": "3.1.0",
  "description": "web3.js as a collection of stores for Svelte, Sapper or SvelteKit.",
  "license": "MIT",
  "repository": "clbrge/svelte-web3",
  "author": {
    "name": "Christophe Le Bars",
    "email": "<clb@toort.net>"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "svelte": "src/stores.js",
  "types": "dist/svelte-web3.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "main": "./dist/index.js",
      "module": "./dist/index.mjs",
      "svelte": "./src/stores.js",
      "types": "./dist/svelte-web3.d.ts"
    },
    "./components": {
      "import": "./src/components/index.js",
      "svelte": "./src/components/index.js"
    }
  },
  "scripts": {
    "update-chains": "node ./scripts/update-chains.js",
    "build": "rollup -c"
  },
  "dependencies": {
    "svelte": "^3.0.0",
    "svelte-proxied-store": "^1.1.3"
  },
  "devDependencies": {
    "eslint-plugin-svelte3": "^3.3.0",
    "release-it": "^14.11.8",
    "rollup": "^2.63.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "sirv-cli": "^2.0.1",
    "svelte": "^3.45.0",
    "web3": "1.6.1"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "sveltekit",
    "svelte",
    "sapper",
    "ethereum",
    "web3",
    "store"
  ],
  "files": [
    "src",
    "dist"
  ]
}
