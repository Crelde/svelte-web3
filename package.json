{
  "name": "svelte-web3",
  "version": "4.1.1",
  "description": "web3.js as a collection of stores for Svelte, Sapper or SvelteKit.",
  "license": "MIT",
  "repository": "clbrge/svelte-web3",
  "author": {
    "name": "Christophe Le Bars",
    "email": "<clb@toort.net>"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/svelte-web3.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "main": "./dist/index.js",
      "module": "./dist/index.mjs",
      "svelte": "./src/stores.js",
      "types": "./dist/svelte-web3.d.ts"
    },
    "./components": {
      "import": "./src/components/index.js",
      "svelte": "./src/components/index.js"
    }
  },
  "scripts": {
    "update-chains": "node ./scripts/update-chains.mjs",
    "format": "prettier --plugin-search-dir . --write .",
    "build": "rollup -c"
  },
  "prettier": "prettier-config-standard",
  "dependencies": {
    "svelte": "^3.0.0",
    "svelte-proxied-store": "^2.0.3"
  },
  "devDependencies": {
    "eslint-plugin-svelte3": "^4.0.0",
    "lint-staged": "^13.2.2",
    "prettier-config-standard": "^5.0.0",
    "release-it": "^15.10.3",
    "rollup": "^3.23.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-svelte": "^7.1.4",
    "svelte": "^3.59.1",
    "web3": "1.10.0"
  },
  "lint-staged": {
    "*.{js,json,md}": "prettier --write"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "sveltekit",
    "svelte",
    "sapper",
    "ethereum",
    "web3",
    "store"
  ],
  "files": [
    "src",
    "dist"
  ]
}
